{
  "name": "@we-learn-js/js-training",
  "version": "0.0.0",
  "description": "JS Training",
  "main": "index.js",
  "scripts": {
    "reset": "npx @s-ui/mono phoenix",
    "build": "parcel build --no-source-maps -d ./public src/index.html && cp ./scripts/fixtures/build/package.json ./public/package.json",
    "serve": "npx serve --single ./public",
    "clear": "rm -Rf .cache dist public",
    "start": "parcel serve -p 3000 src/index.html",
    "dev": "npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "co": "sui-mono commit",
    "lint": "npx @s-ui/lint js --fix",
    "deploy-to-now": "sui-deploy learn-javascript ./public --now",
    "build-deploy-to-now": "npm run wiki:push && npm run build && npm run deploy",
    "wiki:add": "git subtree add --prefix=src/md git@github.com:we-learn-js/js-training.wiki.git master --squash",
    "wiki:pull": "git subtree pull --prefix=src/md git@github.com:we-learn-js/js-training.wiki.git master --squash -m \"Merge changes from origin repository.\"",
    "wiki:push": "git subtree push --prefix=src/md git@github.com:we-learn-js/js-training.wiki.git master"
  },
  "dependencies": {
    "@firebase/app": "0.3.5",
    "@firebase/auth": "0.9.1",
    "@material-ui/icons": "1.0.0-beta.42",
    "classnames": "2.2.6",
    "fast-deep-equal": "2.0.1",
    "material-ui": "1.0.0-beta.42",
    "nanobus": "4.4.0",
    "param-case": "2.1.1",
    "prismjs": "1.15.0",
    "react": "16.7",
    "react-dom": "16.7",
    "react-loadable": "5.5.0",
    "react-markdown": "4.0.4",
    "react-router": "4.3",
    "react-router-dom": "4.3",
    "reveal.js": "3.7.0",
    "three": "0.70.0"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@babel/plugin-proposal-class-properties": "7",
    "@babel/plugin-transform-runtime": "7",
    "@babel/preset-env": "7",
    "@s-ui/deploy": "2.9.0",
    "@s-ui/lint": "2",
    "@s-ui/mono": "1",
    "caniuse-lite": "^1.0.30000989",
    "commitizen": "^2.10.1",
    "fs-extra": "7.0.1",
    "node-sass": "^4.11.0",
    "node-sass-package-importer": "5.2.0",
    "parcel-bundler": "1.11",
    "postcss-cssnext": "3.1.0",
    "postcss-modules": "1.4.1",
    "serve": "^11.1.0"
  },
  "config": {
    "sui-mono": {
      "access": "public",
      "packagesFolder": "./src"
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "author": "David Almeida <davidbarna@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/we-learn-js/js-training.git"
  },
  "bugs": {
    "url": "https://github.com/we-learn-js/js-training/issues"
  },
  "homepage": "https://github.com/we-learn-js/js-training#readme"
}
